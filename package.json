{
  "name": "swifter-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "set": "cp .env.dev .env && docker compose -f docker-compose.yml up",
    "set:down": "docker-compose -f docker-compose.yml down --volumes && docker system prune",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cp .env.dev .env && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "check:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check:type": "tsc --noEmit",
    "check:test": "jest",
    "check": "run-p 'check:*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "makemigrations": "awesome-typeorm makemigrations",
    "migrations": "awesome-typeorm migrations",
    "migrate": "awesome-typeorm migrate",
    "migrate:revert": "awesome-typeorm migrate:revert",
    "eb:prod": "npm install && npm run start:prod"
  },
  "dependencies": {
    "@awesome-dev/server-admin": "^0.0.6",
    "@awesome-dev/server-auth": "^0.1.0",
    "@awesome-dev/server-aws-s3": "^0.0.2",
    "@awesome-dev/server-common": "^0.1.8",
    "@awesome-dev/server-event-emitter": "^0.0.3",
    "@awesome-dev/server-messageme": "^0.0.1",
    "@awesome-dev/server-typeorm": "^0.1.1",
    "@awesome-dev/server-whatsapp": "^0.0.2",
    "@awesome-dev/utils": "^0.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@toss/utils": "^1.4.6",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "2",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.4.2",
    "joi": "^17.12.1",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "@awesome-dev/typings": "^0.0.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engine": "node 20.11.0",
  "packageManager": "npm@10.2.4"
}
